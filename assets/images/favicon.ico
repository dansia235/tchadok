<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <!-- Gradient pour l'icône -->
    <radialGradient id="iconGradient" cx="50%" cy="30%" r="70%">
      <stop offset="0%" style="stop-color:#FFD700"/>
      <stop offset="70%" style="stop-color:#FFA500"/>
      <stop offset="100%" style="stop-color:#FF8C00"/>
    </radialGradient>
    
    <!-- Gradient de fond -->
    <radialGradient id="bgGradient" cx="50%" cy="50%" r="80%">
      <stop offset="0%" style="stop-color:#0066CC"/>
      <stop offset="100%" style="stop-color:#003D7A"/>
    </radialGradient>
    
    <!-- Filtre d'ombre -->
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="1" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Fond circulaire -->
  <circle cx="16" cy="16" r="15" fill="url(#bgGradient)" stroke="#FFD700" stroke-width="0.5"/>
  
  <!-- Note de musique principale -->
  <g transform="translate(8, 8)">
    <!-- Corps de la note -->
    <ellipse cx="6" cy="12" rx="4" ry="3" fill="url(#iconGradient)" filter="url(#glow)"/>
    
    <!-- Tige de la note -->
    <rect x="10" y="6" width="1.5" height="9" fill="url(#iconGradient)" filter="url(#glow)"/>
    
    <!-- Croche -->
    <path d="M11.5,6 Q16,4 16,8 Q16,12 11.5,10 Z" fill="url(#iconGradient)" filter="url(#glow)"/>
  </g>
  
  <!-- Ondes sonores minimalistes -->
  <g stroke="#FFD700" stroke-width="1" fill="none" opacity="0.8">
    <path d="M20,12 Q24,10 24,16 Q24,22 20,20"/>
    <path d="M22,10 Q27,7 27,16 Q27,25 22,22"/>
  </g>
  
  <!-- Petite étoile tchadienne -->
  <polygon points="7,5 7.5,6.5 9,6.5 7.75,7.5 8.25,9 7,8 5.75,9 6.25,7.5 5,6.5 6.5,6.5" 
           fill="#FFD700" opacity="0.7" transform="scale(0.8)"/>
  
  <!-- Accent décoratif -->
  <circle cx="25" cy="25" r="1.5" fill="#FFD700" opacity="0.6"/>
</svg>